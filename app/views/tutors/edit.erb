<%= form_with( model: @tutor ) do | form | %>
    <div>
        <%= form.label "Name" %>
        <%= form.text_field :name %>
    </div>
    <div>
        <%= form.label "Subject" %>
        <%= form.collection_select(:subject_id, Subject.all,:id,:name,prompt: true) %>
    </div>
    <div>
        <%= form.label "Level" %>
        <%= form.collection_select(:level_id, Level.all,:id,:name,prompt: true) %> 
    </div>
     <div class="form-group">
        <%= form.label "Password" %>
        <%= form.password_field :password %>
    </div>
    <div>
        <%= form.label "Bio" %>
        <%= form.text_field :bio %>
    </div>
    <%= form.submit %>
<% end %>